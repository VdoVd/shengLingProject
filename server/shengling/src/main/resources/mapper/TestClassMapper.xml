<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.crm.Mapper.TestClassMapper">
    <select id="all" resultType="com.example.crm.pojo.TestClass">
        select * from TestClass
    </select>

    <insert id="add" parameterType="com.example.crm.pojo.TestClass">
        insert into TestClass values (#{name},#{age})
    </insert>

    <select id="queryByName" resultType="com.example.crm.pojo.TestClass">
        select * from TestClass where name=#{name}
    </select>

</mapper>
